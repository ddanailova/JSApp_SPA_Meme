<div id="meme-feed">
    <h1>Meme Feed</h1>
    <div id="memes">
        {{#if noMemes}}
        <p class="no-memes">No memes in database.</p>
        {{else}}
        {{#each memes}}
        <div class="meme">
            <a href="#" class="meme-title">{{title}}</a>
            <br>
            <a href="#"><img class="meme-image" src="{{imageUrl}}"></a>
            <div class="info">

                <div id="data-buttons">
                    <a href="#/meme/details?id={{_id}}" class="custom-button">Check Out</a>
                    {{#if isCreator}}
                    <a href="#/meme/edit?id={{_id}}" class="custom-button">Edit</a>
                    <a href="#/meme/remove?id={{_id}}" class="custom-button">Delete</a>
                    {{/if}}
                    <a href="#/user/creator/profile?creatorId={{_acl.creator}}" class="creator">Creator: {{creator}}</a>
                </div>
            </div>
            <hr>
        </div>
        {{/each}}
        {{/if}}
    </div>
</div>